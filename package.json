{
  "name": "mmps",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "build": "tsc --sourceMap false && tsc-alias",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --cache --check --ignore-unknown \"src/**/*.ts\"",
    "start": "node dist/main.js",
    "start:dev": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:debug": "node --inspect -r tsconfig-paths/register -r ts-node/register src/main.ts",
    "start:prod": "node dist/main",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "postinstall": "tsc --sourceMap false && tsc-alias",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "validate:cron-names": "node .github/scripts/validate-cron-names.mjs",
    "prepare": "husky install",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.54.0",
    "@langchain/anthropic": "^0.3.26",
    "@langchain/core": "^0.3.58",
    "@langchain/langgraph": "^0.4.5",
    "@langchain/openai": "^0.3.17",
    "@modelcontextprotocol/sdk": "^1.19.1",
    "@modelcontextprotocol/server-github": "^2025.4.8",
    "@pinecone-database/pinecone": "^6.1.2",
    "@supadata/js": "^1.3.0",
    "@vitalets/google-translate-api": "^9.2.0",
    "axios": "^1.11.0",
    "body-parser": "^1.20.2",
    "canvas": "^2.11.2",
    "cheerio": "^1.0.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "googleapis": "^159.0.0",
    "mongodb": "^6.8.0",
    "node-cron": "^4.2.1",
    "node-telegram-bot-api": "^0.66.0",
    "openai": "^5.16.0",
    "pdfreader": "^3.0.7",
    "puppeteer": "^24.22.3",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "telegram": "^2.24.11",
    "ts-node": "^10.9.1",
    "twilio": "^5.7.2",
    "yahoo-finance2": "^2.11.3",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/express": "^4.17.17",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/node-telegram-bot-api": "^0.64.7",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "semantic-release": "^22.0.12",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
