    "@nestjs/core": "^11.1.9",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.2.3",
    "@pinecone-database/pinecone": "^6.1.2",
    "@supadata/js": "^1.3.0",
    "@vitalets/google-translate-api": "^9.2.0",
    "axios": "^1.11.0",
