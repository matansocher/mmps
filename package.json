    "@types/supertest": "^6.0.0",
    "@types/valid-url": "^1.0.7",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "fix-esm-import-path": "^1.10.3",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
