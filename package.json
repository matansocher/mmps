{
  "name": "mmps",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "type": "module",
  "engines": {
    "node": "24.x"
  },
  "scripts": {
    "build": "nest build && fix-esm-import-path dist && node fix-index-imports.mjs",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --cache --check --ignore-unknown \"src/**/*.ts\"",
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:debug:built": "node --inspect dist/main.js",
    "build:watch": "nest build --watch",
    "start:prod": "node dist/main",
    "webhook-proxy": "ngrok http http://localhost:3000",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "postinstall": "tsc --sourceMap false",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "validate:cron-names": "node .github/scripts/validate-cron-names.mjs",
    "prepare": "husky install",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.70.1",
    "@google/generative-ai": "^0.24.1",
